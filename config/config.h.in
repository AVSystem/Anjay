#define ANJAY_MAX_SERVER_PK_OR_IDENTITY_SIZE @MAX_SERVER_PK_OR_IDENTITY_SIZE@
#define ANJAY_MAX_SECRET_KEY_SIZE @MAX_SECRET_KEY_SIZE@

#define ANJAY_MAX_OBSERVABLE_RESOURCE_SIZE @MAX_OBSERVABLE_RESOURCE_SIZE@

